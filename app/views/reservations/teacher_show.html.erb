<div class="container">
  <div class="row">
    <div class="col-12 text-center title">
      <h1>予約詳細画面</h1>
    </div>
    <div class="col-12 mt-3 text-center content">
      <div>
        <label><strong>開始時間</strong></label>
        <%= l @reservation.start_time, format: :short %>
      </div>
      <div>
        <label><strong>終了時間</strong></label>
        <%= l @reservation.end_time, format: :short %>
      </div>
    </div>
    <div class="col-12 text-right">
      <%= link_to '予約画面に戻る', teacher_reservations_index_path %>
    </div>
  </div>
</div>